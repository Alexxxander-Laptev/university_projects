NAME SUMMATION
PUBLIC SUMM
EXTERN data (FIRST_MULT_RESULT_1, FIRST_MULT_RESULT_2, FIRST_MULT_RESULT_3, SECOND_MULT_RESULT_1, SECOND_MULT_RESULT_2, SECOND_MULT_RESULT_3, THIRD_MULT_RESULT_1, THIRD_MULT_RESULT_2, THIRD_MULT_RESULT_3, SUMM_1, SUMM_2, SUMM_3, SUMM_4, SUMM_5, SUMM_6) 
SUMMATION_ROUTINES SEGMENT CODE
RSEG SUMMATION_ROUTINES
JMP summ 

summ:
;яйкюдшбюел лкюдьхи пюгпъд вхякю
MOV R0, #FIRST_MULT_RESULT_3
MOV A, @R0
MOV R0, #SUMM_6
MOV @R0, A

;яйкюдшбюел брнпни пюгпюъд вхякю
MOV R0, #FIRST_MULT_RESULT_2
MOV A, @R0
MOV R0, #SECOND_MULT_RESULT_3
ADDC A, @R0
MOV R0, #SUMM_5
MOV @R0, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_4
MOV @R0, A

;яйкюдшбюел рперхи пюгпъд вхякю
MOV R0, #FIRST_MULT_RESULT_1
MOV A, @R0
MOV R0, #SECOND_MULT_RESULT_2
ADDC A, @R0
MOV B, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_3
MOV @R0, A
MOV A, B
MOV R0, #THIRD_MULT_RESULT_3
ADDC A, @R0
MOV R0, #SUMM_4
ADD A, @R0
MOV @R0, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_3
ADD A, @R0
MOV @R0, A

;яйюкюдшбюел вербпрши пюгпъд вхякю
MOV R0, #FIRST_MULT_RESULT_1 - 1
MOV A, @R0
MOV R0, #SECOND_MULT_RESULT_1
ADDC A, @R0
MOV B, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_2
MOV @R0, A
MOV A, B
MOV R0, #THIRD_MULT_RESULT_2
ADDC A, @R0
MOV B, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_2
ADD A, @R0
MOV @R0, A
MOV A, B
MOV R0, #SUMM_3
ADD A, @R0
MOV @R0, A

;яйкюдшбюел оърши пюгпъд вхякю
MOV R0, #SECOND_MULT_RESULT_1 - 1
MOV A, @R0
MOV R0, #THIRD_MULT_RESULT_1
ADDC A, @R0
MOV R0, #SUMM_2
ADD A, @R0
MOV @R0, A
MOV A, #0
ADDC A, #0
MOV R0, #SUMM_1
MOV @R0, A

;яйкюдшбюел ьеярни пюгпъд вхякю
MOV R0, #THIRD_MULT_RESULT_1 - 1
MOV A, @R0
MOV R0, #SUMM_1
ADD A, @R0
MOV @R0, A

RET
END
















